{
  "presets": [
      [
        "@babel/preset-env",
        {
          "modules": false
        }
      ],
      "@babel/preset-flow"
  ],
  "plugins": [
    "@babel/plugin-transform-runtime"
  ],
  "env": {
    "test": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "modules": "commonjs"
          }
        ]
      ],
      "plugins": [
        "transform-vue-jsx",
        [
          "istanbul",
          {
            "exclude": [
              "**/node_modules/**",
              "**/packages/**",
              "**/test/**",
              "**/Test*"
            ]
          }
        ],
      ]
    }
  },
}
