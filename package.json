{
  "name": "@jnields/vue-helmet",
  "description": "A document head manager for Vue",
  "version": "3.0.0-rc.0",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "files": [
    "dist",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "Joseph Nields <jnields+npm@gmail.com>",
  "contributors": [
    "Joseph Nields <jnields+npm@gmail.com>"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jnields/vue-helmet"
  },
  "keywords": [
    "vue-helmet",
    "vue",
    "document",
    "head",
    "title",
    "meta",
    "link",
    "script",
    "base",
    "noscript",
    "style"
  ],
  "peerDependencies": {
    "vue": "^2.2.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "classnames": "^2.2.6",
    "escape-html": "^1.0.3",
    "exenv": "^1.2.2",
    "to-style": "^1.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^8.0.2",
    "babel-plugin-istanbul": "^5.0.1",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "chai": "^3.5.0",
    "codecov": "^3.1.0",
    "conventional-changelog-cli": "^1.3.1",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.0.0",
    "es6-promise": "^4.2.4",
    "eslint": "^5.5.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "flow-bin": "^0.80.0",
    "istanbul": "^0.4.5",
    "karma": "^3.0.0",
    "karma-chai": "^0.1.0",
    "karma-chai-sinon": "^0.1.5",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-phantomjs-shim": "^1.4.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.30",
    "karma-tap-reporter": "^0.0.6",
    "karma-webpack": "^2.0.9",
    "mocha": "^5.1.1",
    "nyc": "^11.4.1",
    "phantomjs-prebuilt": "^2.1.14",
    "rimraf": "^2.6.1",
    "rollup": "^0.65.2",
    "rollup-plugin-babel": "^4.0.3",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.8.0",
    "vue": "^2.2.1",
    "vue-server-renderer": "^2.5.13",
    "webpack": "^2.2.1"
  },
  "scripts": {
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "clean": "rimraf dist coverage .nyc_output",
    "build": "rollup -c",
    "lint": "eslint --ignore-path .gitignore .",
    "pretest": "npm run lint && flow check",
    "test": "npm run test:browser && npm run test:server",
    "test:browser": "cross-env BABEL_ENV=test karma start karma.config.js",
    "test:server": "cross-env BABEL_ENV=test nyc --reporter json mocha -r @babel/register ./test/server.spec.js",
    "posttest": "istanbul report lcov text",
    "coverage": "istanbul report text-lcov | coveralls",
    "prepublishOnly": "npm t",
    "commit": "git-cz",
    "prepare": "npm run build"
  },
  "nyc": {
    "require": [
      "@babel/register"
    ],
    "sourceMap": false,
    "instrument": false,
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "test/**/*",
      "node_modules/**/*"
    ],
    "all": false,
    "report-dir": "./coverage"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
